{
    for(unsigned i=1;i<=m;++i) cout<<v[i]<<' ';
    cout<<'\n';
}
unsigned x;
void bkt(int k)
{
    for(unsigned i=1;i<=x;++i)
    {
        v[k]=i;
        if(OK(k))
            if(sol(k)) afis(k);
                else bkt(k+1);
    }
}
int main()
{
    cin>>x>>m;
    if(m<=x) bkt(1);
