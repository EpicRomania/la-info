#include <iostream>

using namespace std;
int v[35];
unsigned c;
bool OK(int k)
{
    if(v[1]!=c) return 0;
    if(k>1 && v[k]<=v[k-1]) return 0;
    return 1;
}
unsigned m;
bool sol(int k)
{
    return (k==m);
}
void afis(int k)
{
    for(unsigned i=1;i<=k;++i) cout<<v[i]<<' ';
    cout<<'\n';
}
//sau
void afis()
{
    for(unsigned i=1;i<=m;++i) cout<<v[i]<<' ';
    cout<<'\n';
}
unsigned x;
void bkt(int k)
{
    for(unsigned i=c;i<=x;++i)
    {
        v[k]=i;
        if(OK(k))
            if(sol(k)) afis(k);
                else bkt(k+1);
    }
}
int main()
{
    cin>>x>>m>>c;
    if(m<=x)
        if(c>=1 && c<=x) bkt(1);
            else cout<<c<<"NU este ...";
       else cout<<"NU";
    return 0;
}

