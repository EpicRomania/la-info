#include <iostream>

using namespace std;
int v[35];
unsigned b;
bool OK(int k)
{
  if(v[1]!=b) return 0;
  for(unsigned i=1;i<=k-1;++i)
        if(v[k]==v[i]) return 0;
  return 1;
}
