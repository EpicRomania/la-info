#include <iostream>

using namespace std;
int v[35];
bool OK(int k)
{
    if(k>1 && v[k]<=v[k-1]) return 0;
    return 1;
}
unsigned m;
bool sol(int k)
{
    return (k==m);
}
void afis(int k)
{
    for(unsigned i=1;i<=k;++i) cout<<v[i]<<' ';
    cout<<'\n';
}
//sau
